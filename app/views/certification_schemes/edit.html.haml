.alert.alert-success.alert-dismissible.fade.show#notice{role: 'alert'}
  .text-center= notice
  %button{type: 'button', class: 'close', 'data-dismiss': 'alert', 'aria-label': 'Close'}
    %span{'aria-hidden': 'true'} &times;
.container
  .row
    .col
      = link_to @certification_scheme, class: 'btn btn-outline-info', role: 'button' do
        %i.fa.fa-undo
        Tidak Jadi Ubah
      .card.mt-4.mb-4
        %h5.card-header Tambah Skema Sertifikasi Baru
        .card-body
          = render 'form'
